import java.util.Scanner;
public class Program
{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int k=1;
		for (int i=1;i<=n;i++){
		    for (int j=0;j<i;j++){
		        System.out.print(k+" ");
		        k++;
		    }
		    System.out.println("\n");
		}
	}
}
